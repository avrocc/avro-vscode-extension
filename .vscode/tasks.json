{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "npm: compile",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"compile"
			],
			"problemMatcher": "$tsc",
			"isBackground": false,
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "npm: watch",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"watch"
			],
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"group": {
				"kind": "build"
			}
		}
	]
}
